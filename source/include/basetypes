#pragma once

namespace saturn {

using int8_t  = char;
using int16_t = short int;
using int32_t = int;
using int64_t = long long;

using uint8_t  = unsigned char;
using uint16_t = unsigned short int;
using uint32_t = unsigned int;
using uint64_t = unsigned long long;

using size_t = long unsigned int;

}; // namespace saturn
